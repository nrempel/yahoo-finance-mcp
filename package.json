{
  "name": "yfinance-mcp",
  "version": "1.0.1",
  "description": "MCP server for Yahoo Finance data - stock quotes, historical prices, financials, company info, and news",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "yfinance-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nrempel/yahoo-finance-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/nrempel/yahoo-finance-mcp/issues"
  },
  "homepage": "https://github.com/nrempel/yahoo-finance-mcp#readme",
  "author": "nrempel",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:unit": "vitest --run --exclude '**/*.e2e.test.ts'",
    "test:e2e": "vitest --run src/handlers.e2e.test.ts",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "mcp",
    "mcp-server",
    "model-context-protocol",
    "yahoo-finance",
    "stocks",
    "finance",
    "claude",
    "ai",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "yahoo-finance2": "^2.14.2",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.51.0",
    "vitest": "^2.1.0"
  }
}
